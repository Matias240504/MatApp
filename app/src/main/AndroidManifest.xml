        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Proyecto_prueba"
        tools:targetApi="31">
        <activity
            android:name=".Cliente.AgregarClienteActivity"
            android:exported="false" />
        <activity
            android:name=".Cliente.ListaClienteActivity"
